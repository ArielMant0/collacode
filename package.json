{
  "name": "collacode",
  "version": "0.0.0",
  "scripts": {
    "dev": "cross-env NODE_OPTIONS='--no-warnings' vite --config vite.config.mjs",
    "dev-static": "cross-env NODE_OPTIONS='--no-warnings' vite --config vite.config.static.mjs",
    "build": "vite --config vite.config.mjs build && npm run copyImages",
    "build-static": "vite --config vite.config.static.mjs build && npm run copyImages",
    "copyImages": "node -e \"require('fs').cpSync('./public', './dist', {recursive: true});\"",
    "preview": "vite --config vite.config.mjs preview",
    "preview-static": "vite --config vite.config.static.mjs preview",
    "lint": "eslint . --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "@mdi/font": "7.0.96",
    "@saehrimnir/druidjs": "^0.7.3",
    "@saehrimnir/hagrid": "^0.1.1",
    "@vueuse/core": "^12.5.0",
    "axios": "^1.6.7",
    "chance": "^1.1.12",
    "core-js": "^3.34.0",
    "d3": "^7.9.0",
    "d3-force-boundary": "^0.0.3",
    "d3-sankey": "^0.12.3",
    "file-saver": "^2.0.5",
    "howler": "^2.2.4",
    "js-cookie": "^3.0.5",
    "jszip": "^3.10.1",
    "luxon": "^3.5.0",
    "peerjs": "^1.5.4",
    "roboto-fontface": "*",
    "simplify-js": "^1.2.4",
    "string-comparison": "^1.3.0",
    "uuid": "^11.1.0",
    "vite-plugin-pages": "^0.32.0",
    "vite-plugin-vue-layouts": "^0.11.0",
    "vue": "^3.3.0",
    "vue-toastification": "next",
    "vuetify": "^3.8.4",
    "webrtc-adapter": "^9.0.1"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.2.4",
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-n": "^16.4.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-vue": "^9.19.0",
    "pinia": "^2.1.0",
    "sass": "^1.69.0",
    "unplugin-auto-import": "^0.17.3",
    "unplugin-fonts": "^1.1.0",
    "unplugin-vue-components": "^0.26.0",
    "unplugin-vue-router": "^0.7.0",
    "vite": "^6.3.5",
    "vite-plugin-vuetify": "^2.0.0",
    "vue-router": "^4.2.0"
  }
}
